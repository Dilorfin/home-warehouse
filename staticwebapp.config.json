{
	"routes": [
		{
			"route": "/api/*"
		},
		{
			"route": "*.{js,json,css,svg,jpg,png,ico,woff,woff2}"
		},
		{
			"route": "/*",
			"rewrite": "/index.html"
		}
	],
	"responseOverrides": {
		"401": {
			"rewrite": "/index.html"
		},
		"403": {
			"rewrite": "/index.html"
		},
		"404": {
			"rewrite": "/index.html"
		}
	}
}
